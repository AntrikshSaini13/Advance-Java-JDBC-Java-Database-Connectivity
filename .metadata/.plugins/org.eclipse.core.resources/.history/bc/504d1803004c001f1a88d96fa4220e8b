package JDBC_Static_Query_Execute_By_executeUpdate_Method_DML_Data_Defination_Language;

import java.sql.*;

public class Select_Data_By_DQL_Query {
	public static void main(String[] args) {		
		try {
//			
			Class cl = Class.forName("oracle.jdbc.driver.OracleDriver");
			
//			
			Connection con = DriverManager.getConnection("jdbc:oracle:thin:@localhost:1521:MYSYSTEM", "MADB!!AM", "123");
			
//			
			Statement st = con.createStatement();
			
//			
			ResultSet rs = st.executeQuery("SELECT * FROM STUDENTS");
			
//			
			ResultSetMetaData  rm = rs.getMetaData();			
			int columnNum = rm.getColumnCount();
			for(int i=1; i<=columnNum; i++) {
				System.out.println(rm.getColumnName(i));
			}
			
			
			
		} catch (ClassNotFoundException e) {			
			System.out.println(e);
		} catch (SQLException e) {
			System.out.println(e);
		}
		
	}
}
